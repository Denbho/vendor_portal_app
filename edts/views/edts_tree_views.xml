                    <field name="request_date"/>
                    <field name="edts_company_id" string="Company"/>
                    <field name="company_code"/>
                    <field name="amount"/>
                    <field name="rawland_payment_terms" string="Payment Terms"/>
                    <field name="status" string="Status"/>
                </tree>
            </field>
