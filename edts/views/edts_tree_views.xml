        <record model="ir.ui.view" id="edts_stl_tree_view">
            <field name="name">edts.stl.tree.view</field>
            <field name="model">account.move</field>
            <field name="arch" type="xml">
                <tree string="Subject To Liquidation" create="false">
                    <field name="state" invisible="1"/>
                    <field name="name" string="EDTS NO."/>
                    <field name="request_date"/>
                    <field name="company_id"/>
                    <field name="company_code"/>
                    <field name="requestor"/>
                    <field name="journal_id"/>
                    <field name="currency_id" invisible="1"/>
                    <field name="amount" sum="Amount"/>
                    <field name="liquidation_status"/>
                    <field name="submission_status"/>
                </tree>
            </field>
        </record>
