<data>
    <odoo>
        <record model="ir.ui.view" id="edts_reason_tree_view">
            <field name="name">edts.reason.tree.view</field>
            <field name="model">account.move</field>
            <field name="arch" type="xml">
                <tree string="EDTS Reason">
                    <field name="state" invisible="1"/>
                    <field name="currency_id" invisible="1"/>
                    <field name="name" string="EDTS NO."/>
                    <field name="journal_id"/>
                    <field name="request_date"/>
                    <field name="edts_company_id" string="Company"/>
                    <field name="company_code"/>
                    <field name="reason"/>
                    <field name="amount"/>
                    <field name="status" string="Status"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="edts_purchase_order_tree_view">
